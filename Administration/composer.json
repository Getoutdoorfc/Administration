{
  "name": "getoutdoor/administration",
  "description": "A WordPress plugin for integrating Microsoft services and calendar management",
  "type": "project",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Sune E.",
      "email": "sune@example.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "microsoft/microsoft-graph": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "wp-coding-standards/wpcs": "^3.0",
    "squizlabs/php_codesniffer": "^3.6",
    "phpunit/phpunit": "^9.5",
    "icanhazstring/composer-unused": "^0.8.3"
  },
  "autoload": {
    "psr-4": {
      "Administration\\Includes\\": "Includes/",
      "Administration\\Components\\AdminInterface\\": "Components/AdminInterface/",
      "Administration\\Components\\AdminInterface\\Templates\\": "Components/AdminInterface/Templates",
      "Administration\\Components\\Integrations\\MicrosoftGraph\\": "Components/Integrations/MicrosoftGraph/",
      "Administration\\Components\\Integrations\\Woocommerce\\": "Components/Integrations/Woocommerce/",
      "Administration\\Components\\ProductTypes\\": "Components/ProductTypes/",
      "Administration\\Components\\ProductTypes\\Rentals\\": "Components/ProductTypes/Rentals/",
      "Administration\\Components\\ProductTypes\\Experiences\\": "Components/ProductTypes/Experiences/",
      "Administration\\Components\\Utilities\\": "Components/Utilities/",
      "Administration\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Administration\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "composer dump-autoload -o"
    ],
    "phpcs": "vendor/bin/phpcs",
    "phpstan": "vendor/bin/phpstan analyse",
    "phpunit": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/flex": true
    }
  }
}
